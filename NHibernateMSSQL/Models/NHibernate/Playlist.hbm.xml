<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" auto-import="true" assembly="NHibernateMSSQL" namespace="NHibernateMSSQL.Models">
  <class name="Playlist" table="Playlist" dynamic-update="true" >
    <cache usage="read-write"/>
    <id name="Id" column="PlaylistId">
      <generator class="native" />
    </id>
    <property name="Name" />
    <set name="Tracks" table="PlaylistTrack">
      <key column="PlaylistId"/>
      <many-to-many class="Track" column="TrackId"/>
    </set>
  </class>
</hibernate-mapping>
